#include <iostream>
#include<stdio.h>
#include<string.h>
#include<bits/stdc++.h>
using namespace std;

int main() {
	string A;
    int cnt=0;
    cin>>A;int l=A.length();//cout<<l;
    for(int i=0;i<l;i++)
       cout<<A[i];
    cout<<endl;
    for(int i=0;i<l;i++)
    {
        //cout<<i;
        if(A[i]=='|')
        {
            //cout<<"here";
            cnt++;
        }
    }
    cout<<cnt;
    return 0;
}


